# Database Configuration
POSTGRES_USER=adsb
POSTGRES_PASSWORD=adsb
POSTGRES_DB=adsb_test
DATABASE_URL=postgresql://adsb:adsb@postgres:5432/adsb_test

# Redis Configuration
REDIS_URL=redis://redis:6379/0

# ADS-B Feeder Configuration
ULTRAFEEDER_HOST=ultrafeeder
ULTRAFEEDER_PORT=80
DUMP978_HOST=dump978
DUMP978_PORT=80

# Geographic Location (required for distance calculations)
FEEDER_LAT=48.6969
FEEDER_LON=-122.6767

# Notification Settings
APPRISE_URLS=
NOTIFICATION_COOLDOWN=300

# API Configuration
PORT=5000
POLLING_INTERVAL=2
DB_STORE_INTERVAL=5

# ACARS Configuration
ACARS_ENABLED=true
ACARS_PORT=5555
VDLM2_PORT=5556

# OpenSky Database
OPENSKY_DB_ENABLED=true
OPENSKY_DB_PATH=/data/opensky/aircraft-database.csv

# Python Configuration
PYTHONDONTWRITEBYTECODE=1
PYTHONUNBUFFERED=1

# Mock Services Configuration
ADSBX_API_KEY=

# ACARS Hub Mock Configuration
MOCK_ENABLED=true
MOCK_INTERVAL_MIN=1.0
MOCK_INTERVAL_MAX=2.0
STATION_ID=MOCK-LOCATION-STATION
RELAY_HOST=api
RELAY_PORT=5555
RELAY_PROTOCOL=udp

# Docker Compose Port Mappings
DASHBOARD_PORT=3000
API_PORT=5000
ULTRAFEEDER_PORT_EXTERNAL=8080
DUMP978_PORT_EXTERNAL=8081
ACARSHUB_WEB_PORT=8088

# ACARS Hub UDP Ports
ACARS_UDP_PORT=5550
VDLM2_UDP_PORT=5555
HFDL_UDP_PORT=5556
IMSL_UDP_PORT=5557
IRDM_UDP_PORT=5558

# ACARS Hub TCP Relay Ports
ACARS_TCP_PORT=15550
VDLM2_TCP_PORT=15555
HFDL_TCP_PORT=15556
IMSL_TCP_PORT=15557
IRDM_TCP_PORT=15558

# Development Settings
NODE_ENV=development

# API
PHOTO_CACHE_ENABLED=true
PHOTO_CACHE_DIR=/data/photos
S3_ENABLED=false
S3_BUCKET=skyspy
S3_REGION=us-west-1
S3_ACCESS_KEY=
S3_SECRET_KEY=
S3_ENDPOINT_URL=
S3_PREFIX=airfame-cache
S3_PUBLIC_URL=
