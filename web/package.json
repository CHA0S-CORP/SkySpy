{
  "name": "adsb-dashboard",
  "version": "2.5.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "lint": "eslint src --ext .js,.jsx --report-unused-disable-directives --max-warnings 0",
